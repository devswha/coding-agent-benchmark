{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "coding-agent-benchmark",
  "version": "0.1.0",
  "description": "Benchmark suite for evaluating AI coding agents (Claude Code, Cursor, Aider, OpenHands, etc.)",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "agent-benchmark": "./bin/agent-benchmark"
  },
  "exports": {
    ".": "./src/index.ts",
    "./suites/*": "./src/suites/*.ts",
    "./types": "./src/types.ts",
    "./agent": "./src/agent.ts",
    "./agents": "./src/agents/index.ts"
  },
  "scripts": {
    "dev": "bun run src/cli.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "build": "bun build src/index.ts --outdir dist --target node",
    "list": "bun run src/cli.ts list",
    "leaderboard": "bun run src/cli.ts leaderboard",
    "dashboard": "cd dashboard && bun run dev",
    "dashboard:server": "cd dashboard && bun run server",
    "dashboard:install": "cd dashboard && bun install",
    "dashboard:build": "cd dashboard && bun run build",
    "benchmark": "bun scripts/run-benchmarks.ts",
    "benchmark:all": "bun scripts/run-benchmarks.ts --agent=all",
    "benchmark:easy": "bun scripts/run-benchmarks.ts --difficulty=easy"
  },
  "dependencies": {
    "glob": "^13.0.0",
    "yargs": "^18.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tsconfig/bun": "^1.0.10",
    "@types/bun": "^1.3.5",
    "@types/yargs": "^17.0.33",
    "typescript": "^5.8.2"
  },
  "files": [
    "src",
    "test-projects",
    "bin",
    "dist"
  ],
  "keywords": [
    "benchmark",
    "ai",
    "coding-agent",
    "evaluation",
    "claude-code",
    "cursor",
    "aider",
    "openhands"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devswha/coding-agent-benchmark.git"
  }
}
