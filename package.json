{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "magi-benchmark",
  "version": "0.1.0",
  "description": "Benchmark suite for MAGI - AI coding agent evaluation system",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "magi-benchmark": "./bin/magi-benchmark"
  },
  "exports": {
    ".": "./src/index.ts",
    "./suites/*": "./src/suites/*.ts",
    "./types": "./src/types.ts"
  },
  "scripts": {
    "dev": "bun run src/cli.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "build": "bun build src/index.ts --outdir dist --target node",
    "list": "bun run src/cli.ts list",
    "run:code": "bun run src/cli.ts run code-generation",
    "run:trinity": "bun run src/cli.ts run trinity-protocol",
    "run:task": "bun run src/cli.ts run task-completion",
    "run:agent": "bun run src/cli.ts run agent-comparison",
    "run:all": "bun run src/cli.ts run-all",
    "leaderboard": "bun run src/cli.ts leaderboard",
    "compare": "bun run src/cli.ts compare"
  },
  "peerDependencies": {
    "magi": ">=0.1.0"
  },
  "peerDependenciesMeta": {
    "magi": {
      "optional": true
    }
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.57",
    "@ai-sdk/google": "^2.0.52",
    "@ai-sdk/openai": "^2.0.89",
    "ai": "^5.0.119",
    "glob": "^13.0.0",
    "yargs": "^18.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tsconfig/bun": "^1.0.10",
    "@types/bun": "^1.3.5",
    "@types/yargs": "^17.0.33",
    "typescript": "^5.8.2"
  },
  "files": [
    "src",
    "test-projects",
    "bin",
    "dist"
  ],
  "keywords": [
    "magi",
    "benchmark",
    "ai",
    "coding-agent",
    "evaluation"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devswha/magi-benchmark.git"
  }
}
